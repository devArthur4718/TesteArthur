<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TestForXml" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#311b92</item>
        <item name="colorPrimaryVariant">#6746c3</item>
        <item name="colorOnPrimary">#000063</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#311b92</item>
        <item name="colorSecondaryVariant">#6746c3</item>
        <item name="colorOnSecondary">#000063</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.TestForXml.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.TestForXml.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.TestForXml.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>